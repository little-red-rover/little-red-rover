{
    "names": {
        "rmw_microxrcedds": {
            "cmake-args": [
                "-DRMW_UXRCE_MAX_PUBLISHERS=4"
                "-DRMW_UXRCE_MAX_HISTORY=16",
                "-DRMW_UXRCE_STREAM_HISTORY=16",
                "-DRMW_UXRCE_MAX_TRANSPORT_MTU=4096",
                "-DRMW_UXRCE_ENTITY_CREATION_DESTROY_TIMEOUT=0"
            ]
        },
        "microxrcedds_client": {
            "cmake-args": [
                "-DUCLIENT_HARD_LIVELINESS_CHECK=ON",
                "-DUCLIENT_HARD_LIVELINESS_CHECK_TIMEOUT=5000",
                "-DUCLIENT_UDP_TRANSPORT_MTU=4096",
                "-DUCLIENT_CUSTOM_TRANSPORT_MTU=4096",
                "-DUCLIENT_MAX_SESSION_CONNECTION_ATTEMPTS=3"
            ]
        }
    }
}
